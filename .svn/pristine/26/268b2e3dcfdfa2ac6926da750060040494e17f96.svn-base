package br.gov.ms.defensoria.comum.mail;

/**
 * 
 * @author Rafael Gouveia da Silva
 * @since JDK7
 * @version 1.0
 * 
 **/
public class SimpleUsuarioMail {
	private String email;
	private String nome;

	public SimpleUsuarioMail() {

	}

	public SimpleUsuarioMail(String email, String nome) {
		super();
		this.email = email;
		this.nome = nome;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

}
