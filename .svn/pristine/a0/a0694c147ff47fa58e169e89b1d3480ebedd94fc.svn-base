public class SimpleDefensorComarca {
	
	private Long idPessoa;
	private Long idFuncionario;
	private String nomeDefensor;	
	private String nomeComarca;
	private Integer idAdmissao;
	
	
	public SimpleDefensorComarca(Long idPessoa, Long idFuncionario, String nomeDefensor, String nomeComarca, Integer idAdmissao) {		
		this.idPessoa = idPessoa;
		this.idFuncionario = idFuncionario;
		this.nomeDefensor = nomeDefensor;
		this.nomeComarca = nomeComarca;
		this.idAdmissao = idAdmissao;
	}
	
	public Long getIdPessoa() {
		return idPessoa;
	}
	public void setIdPessoa(Long idPessoa) {
		this.idPessoa = idPessoa;
	}
	public Long getIdFuncionario() {
		return idFuncionario;
	}
	public void setIdFuncionario(Long idFuncionario) {
		this.idFuncionario = idFuncionario;
	}
	public String getNomeDefensor() {
		return nomeDefensor;
	}
	public void setNomeDefensor(String nomeDefensor) {
		this.nomeDefensor = nomeDefensor;
	}
	public String getNomeComarca() {
		return nomeComarca;
	}
	public void setNomeComarca(String nomeComarca) {
		this.nomeComarca = nomeComarca;
	}
	public Integer getIdAdmissao() {
		return idAdmissao;
	}
	public void setIdAdmissao(Integer idAdmissao) {
		this.idAdmissao = idAdmissao;
	}
}	
